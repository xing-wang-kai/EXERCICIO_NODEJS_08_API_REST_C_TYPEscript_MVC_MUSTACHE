        {{/products}}
    </ul>
    <hr>
    <ul>
        {{#frazesdoDIa}}
            <li>{{.}}</li>
        {{/frazesdoDIa}}
        {{^frazesdoDIa}}
            <p>Não foi encontrados frazes do dia</p>
        {{/frazesdoDIa}}
    </ul>
    
{{>partials/footer}}