    {{ #showOld }}
        <p>Você é bem velho tem {{ user.idade }} </p>
    {{ /showOld }}

    <ul>
        {{#products}}
            <li>O produto: {{ title }} tem o preço R$ {{ price }} </li>
        {{/products}}
